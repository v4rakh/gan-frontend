{
  "name": "gan-frontend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "scripts": {
    "dev": "chmod +x ./env.sh && ./env.sh && mv env-config.js ./public/ && react-scripts start",
    "prod": "npm run clean && npm run build",
    "build": "NODE_ENV=production react-scripts build && cp env.sh .env build/",
    "build:docker": "NODE_ENV=production sudo docker build --no-cache -t v4rakh/gan-frontend:latest -t gan-frontend:latest .",
    "clean": "rm -rf build/",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage --passWithNoTests",
    "eject": "react-scripts eject",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "axios": "0.19.2",
    "axios-hooks": "2.1.0",
    "bootstrap": "4.5.2",
    "history": "4.10.1",
    "i18next": "19.7.0",
    "jquery": "3.5.1",
    "lodash": "4.17.20",
    "popper.js": "1.16.1-lts",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-bootstrap": "1.3.0",
    "react-dom": "16.13.1",
    "react-i18next": "11.7.1",
    "react-modal": "3.11.2",
    "react-modal-hook": "3.0.0",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-table": "7.5.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "prettier": "2.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
