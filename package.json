{
  "name": "gan-frontend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^14",
    "npm": "^6"
  },
  "scripts": {
    "dev": "chmod +x ./env.sh && ./env.sh && mv env-config.js ./public/ && react-scripts start",
    "prod": "npm run clean && npm run build",
    "build": "NODE_ENV=production react-scripts build && cp env.sh .env build/",
    "build:docker": "NODE_ENV=production sudo docker build --no-cache -t varakh/gan-frontend:latest .",
    "clean": "rm -rf build/",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage --passWithNoTests",
    "eject": "react-scripts eject",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.7.0",
    "axios": "0.21.1",
    "axios-hooks": "2.3.0",
    "bootstrap": "4.6.0",
    "history": "4.10.1",
    "i18next": "19.8.7",
    "jquery": "3.5.1",
    "lodash": "4.17.20",
    "popper.js": "1.16.1-lts",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-bootstrap": "1.4.3",
    "react-dom": "17.0.1",
    "react-i18next": "11.8.6",
    "react-modal": "3.12.1",
    "react-modal-hook": "3.0.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.2",
    "react-table": "7.6.3",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.12.16",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "prettier": "2.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
